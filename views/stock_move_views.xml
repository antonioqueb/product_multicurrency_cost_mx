<odoo>
    <record id="view_move_tree_valuation_usd" model="ir.ui.view">
        <field name="name">stock.move.tree.valuation.usd</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree"/>
        <field name="arch" type="xml">
            <!-- Insertamos despuÃ©s del precio unitario nativo -->
            <xpath expr="//field[@name='price_unit']" position="after">
                <field name="usd_unit_cost" string="Costo Unit. USD" optional="show"/>
                <field name="usd_value" string="Valor Total USD" optional="show" sum="Total USD"/>
            </xpath>
        </field>
    </record>
</odoo>